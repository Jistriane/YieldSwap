{
  "name": "@yieldswap/sdk",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@stellar/stellar-sdk": "^11.1.0",
    "axios": "^1.6.2",
    "bignumber.js": "^9.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "eslint": "^8.42.0",
    "eslint-config-custom": "*",
    "tsup": "^6.0.1",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
} 